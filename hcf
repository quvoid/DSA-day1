public class HCF {
    public static int hcf(int a, int b) {
        if (b == 0) {
            return a; 
        } else {
            return hcf(b, a % b);  
        }
    }

    public static void main(String[] args) {
        int num1 = 56;
        int num2 = 32;
        int result = hcf(num1, num2);
        
        System.out.println("HCF of " + num1 + " and " + num2 + " is: " + result);
    }
}
